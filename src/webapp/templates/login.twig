{% extends 'base.twig' %}



{% block content %}

<h2>Login</h2>

{{ flash.error|raw }} <br />

<form action="" method="POST">
    <label>Username: </label>
    <input type="text" name="username" minlength="1" required> <br />
    <label>Password: </label>
    <input type="password" name="password" required> <br />
    <div class="g-recaptcha" data-sitekey="6LdvHZkUAAAAAP8a2tV-aMp-5G3O1wmHyrx3xccJ"></div>
    <input type="submit" name="submit" value="Log in">
</form>
{% endblock %}

